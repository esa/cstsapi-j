	 	 	 ,	 rate2Over3          	 	 (1)
	 	 	 ,	 rate3Over4          	 	 (2)
	 	 	 ,	 rate5Over6          	 	 (3)
	 	 	 ,	 rate7Over8          	 	 (4)
	 	 	 }

	 	 }

,	 	 reedSolomon         	 [2]	 	 SEQUENCE
